    "e2e": "ng e2e",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "compile:server": "tsc -p server.tsconfig.json",
    "serve:ssr": "node dist/server",
    "express": "node ./express.js",
    "clean": "rimraf ./dist/",
    "gcloud": "gcloud app deploy"
